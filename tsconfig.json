{
  "compilerOptions": {
    "target": "es5",
    "outDir": "dist",
    "skipLibCheck": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react",
    "lib": ["es2015", "dom"],
  },
  "include": [
    "src"
  ]
}
